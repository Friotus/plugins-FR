{
  "headers": [
    {
      "source": "**/*.@(html|json)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "http://localhost:3001"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Origin, X-Requested-With, Content-Type, Accept, Range"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ]
}
